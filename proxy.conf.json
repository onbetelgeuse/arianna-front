{
    "/api/*": {
      "target": "http://localhost:3001/",
      "secure": false,
      "changeOrigin": true,
      "pathRewrite": {
        "^/api": "api"
      }
    },
    "/socket.io/*": {
      "target": "http://localhost:3000/",
      "changeOrigin": true,
      "secure": false,
      "ws": true
    }
  }